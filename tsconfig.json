{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "es6"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "downlevelIteration": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/components/KitchenFeed.tsx",
    "src/components/OptimizedRecipeFeed.tsx",
    "src/components/OptimizedRecipeCard.tsx",
    "src/components/EnhancedRecipeReel.tsx",
    "src/components/RecipeFeed.tsx",
    "src/components/TikTokStyleFeed.tsx",
    "src/components/Community.tsx",
    "src/components/ContentManager.tsx",
    "src/components/CreatorDashboard.tsx",
    "src/components/MealPlanner.tsx",
    "src/components/RestaurantHub.tsx",
    "src/components/SmartPantry.tsx",
    "src/components/DesignSystemDemo.tsx",
    "src/components/MotionSystem.tsx",
    "src/components/AdvancedAnimations.tsx",
    "src/components/WebGLMotionSystem.tsx",
    "src/components/MonetizationTools.tsx",
    "src/components/CreatorProfile.tsx",
    "src/components/RecipeReel.tsx",
    "src/components/RecipeVideoCard.tsx"
  ]
}
