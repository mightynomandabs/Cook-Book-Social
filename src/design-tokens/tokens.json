{
  "brand": {
    "personality": "warm, appetizing, high-energy",
    "promise": "foodies find their feed",
    "colors": {
      "primary": {
        "1": "#FF7A00",
        "2": "#FF3D71", 
        "3": "#FFD166"
      },
      "gradients": {
        "conic": "conic-gradient(from 0deg, #FF7A00, #FF3D71, #FFD166, #FF7A00)",
        "linear": "linear-gradient(135deg, #FF7A00 0%, #FF3D71 50%, #FFD166 100%)",
        "radial": "radial-gradient(ellipse at center, #FF7A00 0%, #FF3D71 50%, #FFD166 100%)"
      }
    },
    "neutrals": {
      "ink": {
        "900": "#171717",
        "600": "#5B5B5B", 
        "400": "#9B9B9B",
        "200": "#EAEAEA"
      },
      "surface": "#FFFFFF",
      "elevated": "rgba(12, 12, 12, 0.8)"
    },
    "semantics": {
      "success": "#2CC36B",
      "warning": "#FFB020",
      "danger": "#EF4444"
    }
  },
  "typography": {
    "fonts": {
      "display": "Poppins",
      "ui": "Inter"
    },
    "weights": {
      "display": [600, 700],
      "ui": [400, 500, 600]
    },
    "scales": {
      "h1": { "size": "32px", "lineHeight": "40px" },
      "h2": { "size": "24px", "lineHeight": "32px" },
      "h3": { "size": "20px", "lineHeight": "28px" },
      "body": { "size": "16px", "lineHeight": "24px" },
      "caption": { "size": "12px", "lineHeight": "16px" }
    }
  },
  "spacing": {
    "xs": "4px",
    "sm": "8px", 
    "md": "16px",
    "lg": "24px",
    "xl": "32px",
    "2xl": "48px",
    "3xl": "64px"
  },
  "radii": {
    "xs": "6px",
    "sm": "10px",
    "md": "16px", 
    "lg": "24px",
    "xl": "28px",
    "full": "50%"
  },
  "shadows": {
    "low": "0 1px 2px rgba(0,0,0,0.06)",
    "mid": "0 6px 20px rgba(16,17,20,0.18)",
    "high": "0 14px 40px rgba(16,17,20,0.28)",
    "glow": "0 0 20px rgba(255,122,0,0.3)"
  },
  "motion": {
    "curves": {
      "global": "cubic-bezier(0.2, 0.8, 0.2, 1)",
      "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
      "spring": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
    },
    "durations": {
      "xs": "120ms",
      "s": "200ms", 
      "m": "300ms",
      "l": "600ms",
      "xl": "800ms"
    },
    "stagger": {
      "min": "30ms",
      "max": "60ms"
    }
  },
  "performance": {
    "webgl": {
      "maxCanvases": 2,
      "maxDrawCalls": 150,
      "maxParticles": 300,
      "targetFPS": 60,
      "fallbackFPS": 40
    }
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  }
}
